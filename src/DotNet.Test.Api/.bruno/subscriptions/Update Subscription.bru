
meta {
  name: Update Subscription
  type: http
  seq: 4
}

put {
  url: {{base_url}}/subscriptions/{{subscription_id}}
  body: json
  auth: none
}

body:json {
  "id": "{{subscription_id}}",
  "accountId": "acc_updated",
  "type": "premium_plus",
  "status": "inactive",
  "payer": {
    "name": "Jane Doe",
    "email": "jane.doe@example.com",
    "taxId": "00987654321",
    "mobile": "+5511912345678"
  },
  "availablePaymentMethods": [
    "bank_transfer"
  ],
  "intervalMultiplier": 3
}

vars {
  base_url: http://localhost:5079
  subscription_id: 
}


